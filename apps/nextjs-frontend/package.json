{
  "name": "nextjs-frontend",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=9"
  },
  "scripts": {
    "build": "next build",
    "build:icons": "tsx src/assets/iconify-icons/bundle-icons-css.ts",
    "postinstall": "echo 'Skipping icon build during install'",
    "start:dev": "next dev",
    "start:prod": "next start",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "docker:build": "cd ../.. && docker build -t nextjs-frontend -f apps/nextjs-frontend/dockerfile .",
    "docker:start:dev": "docker run -p 3000:3000 --env-file .env nextjs-frontend"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "5.1.1",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@mui/material-nextjs": "^5.15.11",
    "@tanstack/react-query": "5.80.6",
    "@tanstack/react-query-devtools": "5.80.6",
    "apexcharts": "^4.7.0",
    "classnames": "^2.5.1",
    "dotenv": "16.5.0",
    "jsdom": "26.1.0",
    "next": "15.3.3",
    "primeicons": "7.0.0",
    "primereact": "10.9.6",
    "react": "19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.57.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-use": "^17.6.0",
    "server-only": "^0.0.1",
    "uuid": "11.1.0",
    "zod": "3.25.58",
    "zustand": "5.0.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.218",
    "@iconify/tools": "^4.0.4",
    "@iconify/types": "^2.0.0",
    "@iconify/utils": "^2.1.24",
    "@playwright/test": "1.53.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.31",
    "@types/react": "19.1.7",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "autoprefixer": "^10.4.21",
    "eslint": "9.28.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "postcss": "8.5.4",
    "postcss-nesting": "^13.0.2",
    "prettier": "3.5.3",
    "stylelint": "^16.20.0",
    "stylelint-use-logical-spec": "^5.0.1",
    "tailwindcss": "4.1.8",
    "tsx": "^4.20.0",
    "typescript": "5.8.3",
    "xo": "1.1.0"
  }
}